<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vboxprincipal" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="350.0" prefWidth="425.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.InterfazInicioSesionController">
   <children>
      <Label fx:id="labelinicio" alignment="CENTER" contentDisplay="CENTER" text="Iniciar Sesión">
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
      <VBox fx:id="vboxcampos" prefHeight="159.0" prefWidth="425.0">
         <children>
            <Label fx:id="labelusuario" text="Usuario">
               <font>
                  <Font name="Arial Black" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Label>
            <TextField fx:id="textuser" onKeyTyped="#eventKey" promptText="Ingrese su usuario">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Label fx:id="labelcontra" text="Contraseña">
               <font>
                  <Font name="Arial Black" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <PasswordField fx:id="textcontra" onKeyTyped="#eventKey" promptText="Digite la clave">
               <font>
                  <Font size="18.0" />
               </font>
            </PasswordField>
         </children>
         <VBox.margin>
            <Insets left="30.0" right="30.0" />
         </VBox.margin></VBox>
      <Button fx:id="btninicio" maxWidth="-Infinity" mnemonicParsing="false" onAction="#eventAction" text="Iniciar sesión">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
